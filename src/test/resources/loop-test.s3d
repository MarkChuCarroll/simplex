// Simple test of iteration.
fun cubish(length: Float, width: Float, height: Float, x: Float): Csg do
    block(length, width, height)->move_to(x, x, x*2.0)
end

fun make_a_bunch(): [Csg] do
  for m in 1->to(4) do
      cubish(m->float()*10.0, m->float()*5.0, m->float()*2.0, m->float()*10.0)
  end
end

val list: [Csg] = make_a_bunch()

produce(a) do
   list[0]
   list[1]
   list[2]
   list[3]
   print([list[0]->bounds(), list[1]->bounds()])
end
